{
  "name": "@tpc/app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rm -rf ./dist",
    "clean:all": "rm -rf ./dist; rm -rf ./node_modules",
    "db:all": "npm run db:generate && npm run db:migrate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "dev": "vite",
    "format": "prettier --cache --check --write .",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "start": "export NODE_ENV='production'; node --loader ts-node/esm --no-warnings ./src/server.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "1.14.1",
    "@hono/trpc-server": "^0.3.4",
    "@hono/vite-dev-server": "^0.19.0",
    "@tpc/schemas": "workspace:*",
    "@trpc/client": "^11.1.1",
    "@trpc/server": "^11.1.1",
    "@types/ws": "^8.18.1",
    "better-auth": "^1.2.7",
    "daisyui": "^5.0.27",
    "dotenv": "16.5.0",
    "drizzle-orm": "0.43.1",
    "drizzle-zod": "0.7.1",
    "hono": "4.7.8",
    "hono-pino": "^0.8.0",
    "i18next": "^24.2.3",
    "lucide-react": "^0.484.0",
    "pg": "^8.14.1",
    "pinia": "^3.0.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-router": "7.4.0",
    "react-use": "^17.6.0",
    "stoker": "^1.4.2",
    "tailwindcss": "^4.1.4",
    "ws": "^8.18.2",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.26.0",
    "@tailwindcss/vite": "^4.0.17",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.13",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "0.31.1",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.31.1",
    "vite": "^6.3.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
